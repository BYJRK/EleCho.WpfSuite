<Application x:Class="WpfTest.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ws="https://github.com/OrgEleCho/EleCho.WpfSuite"
             xmlns:wssd="https://github.com/OrgEleCho/EleCho.WpfSuite/SimpleDesign"
             xmlns:wsfd="https://github.com/OrgEleCho/EleCho.WpfSuite/FluentDesign"
             xmlns:utils="clr-namespace:WpfTest.Utilities"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <wsfd:FluentDesignResources/>
                <!--<wssd:SimpleDesignResources/>-->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
